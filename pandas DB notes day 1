    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "CSV =\"salaries_by_college_major.csv\"\n",
        "df = pd.read_csv(CSV)\n",
        "clean_df = df.dropna()\n",
        "\n",
        "startmed = clean_df['Mid-Career Median Salary']\n",
        "startmaj = clean_df['Undergraduate Major']\n",
        "allist = [item for item in startmed]\n",
        "top5 = []\n",
        "top5maj = []\n",
        "while len(top5) < 5:\n",
        "  fort = max(allist)\n",
        "  fortindex = allist.index(fort)\n",
        "  top5.append(fort)\n",
        "  top5maj.append(startmaj[fortindex])\n",
        "  allist.pop(fortindex)\n",
        "\n",
        "top5dic = dict(zip(top5maj, top5))\n",
        "print(top5dic)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
