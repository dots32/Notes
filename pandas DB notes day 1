      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "CSV =\"salaries_by_college_major.csv\"\n",
        "df = pd.read_csv(CSV)\n",
        "clean_df = df.dropna()\n",
        "pd.options.display.float_format = '{:,.2f}'.format\n",
        "\n",
        "numeric_columns = clean_df.select_dtypes(include=['number']).columns\n",
        "grouped_df = clean_df.groupby('Group')[numeric_columns].mean()\n",
        "grouped_df.head()\n",
        "\n"
